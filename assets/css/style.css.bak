 .signup-form input[type="text"],

.signup-form input[type="email"],

.signup-form input[type="submit"] {

  height: 25px !important;

  font-size: 14px !important;

  text-transform: uppercase;

  display: inline-block !important;

  font-family: "Open Sans" !important;

  font-weight: 100 !important;

  border: none !important;

  border-bottom: 2px solid #ccc !important;

  outline: none !important;

  color: #555 !important;

  padding: 3px !important;

  width: 200px !important;

  box-shadow: none !important;

  top: 0 !important;

  right: 0 !important;

  background: none !important;

  z-index: 3 !important;

  transition: width .4s cubic-bezier(0.000, 0.795, 0.000, 1.000) !important;

  cursor: pointer !important;

}

.signup-form input[type="submit"]{

	font-weight: bold !important;

	font-size: 14px !important;

}

.signup-form input[type="submit"]:hover{

	border-bottom: 2px solid #158be6 !important;

}



.signup-form input[type="text"]:hover,

.signup-form input[type="email"]:hover{

  border-bottom: 2px solid #158be6 !important;

  cursor: text !important;

}



.signup-form input[type="text"]:focus,

.signup-form input[type="email"]:focus,

.signup-form input[type="submit"]:focus{

  z-index: 1 !important;

  border-bottom: 2px solid #158be6 !important;

  cursor: text !important;

}